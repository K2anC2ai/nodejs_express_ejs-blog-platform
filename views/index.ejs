<%- include('header') %>
<body>
<div class="content">
    <h1>My blog</h1>
    <div class="posts">
    <% posts.forEach(post => {%>
        <div class="post">
            <h2><a style="font-size: 30px;" href="/post/<%= post.id %>"><%= post.title %></a></h2>
        </div>    
    <% }) %>
</div>
</div>
</body>
<%- include('footer') %>